{
	"Header": "Dev hui test\n",

	"Pager": {
		"Title": "HUI - Feedback"
	},

	"Keys": {
		"Execute": "L"
	},

	"Entry": {
		"Alignment": "left",
		"MenuPrefix": "> [",
		"MenuPostfix": "]",
		"MenuHoverPrefix": "-> [",
		"MenuHoverPostfix": "]",
		"ShellPrefix": "> ",
		"ShellPostfix": "",
		"ShellHoverPrefix": "-> ",
		"ShellHoverPostfix": "",
		"ShellSessionPrefix": "> <",
		"ShellSessionPostfix": ">",
		"ShellSessionHoverPrefix": "-> <",
		"ShellSessionHoverPostfix": ">",
		"GoPrefix": "> !",
		"GoPostfix": "",
		"GoHoverPrefix": "-> !",
		"GoHoverPostfix": "",

		"Fg": {
			"Active": false,
			"R": 0,
			"G": 0,
			"B": 0
		},

		"Bg": {
			"Active": false,
			"R": 0,
			"G": 0,
			"B": 0
		},

		"HoverFg": {
			"Active": true,
			"R": 0,
			"G": 0,
			"B": 0
		},

		"HoverBg": {
			"Active": true,
			"R": 255,
			"G": 255,
			"B": 255
		}
	},

	"Events": {
		"Start": "Welcome",
		"Quit": "Goodbye"
	},

	"Menus": {
